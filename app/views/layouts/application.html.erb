<!DOCTYPE html>
<html>
  <head>
    <title>Baseball</title>
    
   
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

   
   

<%= stylesheet_link_tag 'https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css', integrity: 'sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm', crossorigin: 'anonymous' %>
<%= javascript_include_tag 'https://code.jquery.com/jquery-3.2.1.slim.min.js', integrity: 'sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN', crossorigin: 'anonymous' %>
<%= javascript_include_tag 'https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js', integrity: 'sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q', crossorigin: 'anonymous' %>
<%= javascript_include_tag 'https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js', integrity: 'sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl', crossorigin: 'anonymous' %>
    <%= javascript_pack_tag "application" %>
    <%= favicon_link_tag asset_path('logo.png') %>

  </head>


  <body>

  <%= render partial: 'layouts/navbar' %>
  
<br />
<% if controller_name == 'tournaments' && action_name.in?(['new', 'edit' ,'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'verify' && action_name.in?(['new' ,'edit','create']) %>
  <%= yield %>
  <% elsif controller_name == 'teams' && action_name.in?(['new', 'edit' ,'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'opponent_teams' && action_name.in?(['new', 'edit' ,'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'registrations' && action_name.in?(['new','edit', 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'sessions' && action_name.in?(['new','edit','create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'roosters' && action_name.in?(['new','edit' , 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'opponent_roosters' && action_name.in?(['new','edit' , 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'events' && action_name.in?(['new','edit' , 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'team_line_ups' && action_name.in?(['new','edit' , 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'opponent_team_line_ups' && action_name.in?(['new','edit' , 'create','show','index']) %>
  <%= yield %>
  <% elsif controller_name == 'users' && action_name.in?(['index']) %>
  <%= yield %>

  <% else %>
  <%= render partial: 'layouts/tables' %>
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
        <%= yield %>
<% end %>


</body>
<%= render partial: 'layouts/footer' %>
</html>
